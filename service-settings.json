{
	"kind": "Service",
	"apiVersion": "v1",
	"metadata": {
		"name": "teamcity-service"
	},
 	"spec": {
		"ports": [{
			"name": "http",
			"port": 80,
			"targetPort": 8111	
		}, {
			"name": "agent",
			"port": 9090,
			"targetPort": 9090	
		}],
		"selector": {
      		"server": "prod",
			"app": "teamcity"
    	},
    	"type": "LoadBalancer"
	}
}
